      <exercise>
        <title>Ejercicio 2</title>
        <webwork-reps xml:id="extracted-webwork-9" ww-id="webwork-9">
    <authored>
      
        <pg-macros>
                              <macro-file>contextLimitedPowers.pl</macro-file>
                              <macro-file>PGunion.pl</macro-file>
                              <macro-file>contextLimitedNumeric.pl</macro-file>
                          </pg-macros>
        <setup><pg-code>
                                  do {
                                  $b = random(2,6,1);
                                  $a = random(3,7,1)*$b;
                                  $c = random(1,7,1)*$b;
                                  } until (($a/$c != 1) and (gcd($a,$c) !=$c));
                                  $m1 = random(2,20,1);
                                  do {$n1 = random(4,20,1)} until ($n1 != $m1);
                                  do {$p1 = random(5,20,1)} until ($p1 != $m1 and $p1 != $n1);
      
                                  Context("Numeric")-&gt;variables-&gt;are(x=&gt;"Real");
                                  $var = "x";
      
                                  LimitedPowers::OnlyPositiveIntegers();
                                  $frac = "\frac{\left($a $var^{$m1}\right)\left($var^{$n1}\right)}{$c $var^{-$p1}}";
      
                                  #Context(LimitedPowers::OnlyPositiveIntegers());
                                  $numconst = $a/gcd($a,$c);
                                  $denconst = $c/gcd($a,$c);
                                  $numexp1 = ($m1+$n1+$p1);
                                  $den = Formula("$denconst ")-&gt;reduce;
                                  $num = Formula("$numconst $var^{$numexp1}")-&gt;reduce;
                                  Context()-&gt;texStrings;
                                  if ($denconst == 1) {
                                    $texanswer = "\( $num  } \)";
                                  } else {
                                    $texanswer = "\(\displaystyle \frac{$num }{$denconst  } \)";
                                  }
      
                                  $multians = MultiAnswer($num, $den)-&gt;with(
                                    singleResult =&gt; 0,
                                    allowBlankAnswers =&gt; 1,
                                    checker =&gt; sub {
                                        my ( $correct, $student, $self ) = @_;
                                        my ( $f1stu, $f2stu ) = @{$student};
                                        my ( $f1, $f2 ) = @{$correct};
                                   
                                        if ( ( $f1==$f1stu and  $f2==$f2stu) ) {
                                            return [1,1];
                                        } elsif ( $f1==$f1stu ) {
                                            return [1,0];
                                        } elsif ( $f2==$f2stu ) {
                                            return [0,1];
                                        } elsif ( $f1*$f2stu==$f1stu*$f2 ) {
                                            $self-&gt;setMessage(1,"Simplify your answer further");
                                            $self-&gt;setMessage(2,"Simplify your answer further");
                                            return [0,0];
                                        } else {
                                            return [0,0];
                                        }
                                    }
                                  );
                                  Context()-&gt;operators-&gt;undefine("+","-","/");
                              </pg-code></setup>
        <statement><p>Escribe la fracciOn usando exponentes positivos solamente. Simplifica to respuesta al mAximo.
                              <me><var name="$frac"/></me></p><p> <m><var name="$frac"/>= </m><var name="$multians" width="5"/>/<var name="$multians" width="5"/></p></statement>
      
    </authored>

      
