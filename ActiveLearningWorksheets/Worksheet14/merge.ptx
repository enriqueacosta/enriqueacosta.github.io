                                      $x = Formula("2*cos(t)+1");
                                      $y = Formula("2*sin(t)-2");
                                      $t0 = Compute("0");
                                      $t1 = Compute("pi");
      
                                      ($x0,$y0) = (3,-2);
                                      ($x1,$y1) = (-1,-2);
      
                                      $multians = MultiAnswer($x, $y, $t0, $t1)-&gt;with(
                                        singleResult =&gt; 0,
                                        checker =&gt; sub {
                                            my ( $correct, $student, $self ) = @_;
                                            my ( $xstu, $ystu, $t0stu, $t1stu ) = @{$student};
                                            if ( 
                                                 ( (($xstu-1)**2 + ($ystu+2)**2) == 4 )  and
                                                 ( ($xstu-&gt;eval(t=&gt;$t0stu)) == $x0 ) and
                                                 ( ($ystu-&gt;eval(t=&gt;$t0stu)) == $y0 ) and
                                                 ( ($xstu-&gt;eval(t=&gt;$t1stu)) == $x1 ) and
                                                 ( ($ystu-&gt;eval(t=&gt;$t1stu)) == $y1 )
                                               ) {
                                                  return [1,1,1,1];
      
                                            } elsif (
                                                 ( (($xstu-1)**2 + ($ystu+2)**2) == 4 )  and
                                                 ( ($xstu-&gt;eval(t=&gt;$t0stu)) == $x0 ) and
                                                 ( ($ystu-&gt;eval(t=&gt;$t0stu)) == $y0 ) 
                                               ) { 
                                                  return [1,1,1,0];
      
                                            } elsif (
                                                 ( (($xstu-1)**2 + ($ystu+2)**2) == 4 )  and
                                                 ( ($xstu-&gt;eval(t=&gt;$t1stu)) == $x1 ) and
                                                 ( ($ystu-&gt;eval(t=&gt;$t1stu)) == $y1 )
                                               ) { 
                                                  return [1,1,0,1];
      
                                            } elsif (
                                                 ( (($xstu-1)**2 + ($ystu+2)**2) == 4 ) 
                                               ) { 
                                                  return [1,1,0,0];
      
                                            } else {
                                                  return [0,0,0,0];
                                            }
                                        }
                                      );
